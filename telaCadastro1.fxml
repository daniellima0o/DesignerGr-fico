<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/20.0.1" fx:controller="com.daniellima.login.controller.TelaCadastroController">
    <children>
        <Label fx:id="usuarioLabel" layoutX="239.0" layoutY="55.0" text="Nome de Usuário">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <TextField fx:id="usuarioTextField" layoutX="201.0" layoutY="80.0" promptText="Nome de usuário">
            <font>
                <Font size="16.0" />
            </font>
        </TextField>
        <Label fx:id="senhaLabel" layoutX="278.0" layoutY="115.0" text="Senha">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <PasswordField fx:id="senhaField" layoutX="201.0" layoutY="140.0" promptText="senha">
            <font>
                <Font size="16.0" />
            </font>
        </PasswordField>
        <Label fx:id="confirmarSenhaLabel" layoutX="242.0" layoutY="175.0" text="Confirmar senha">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <PasswordField fx:id="confirmarSenhaField" layoutX="201.0" layoutY="200.0" promptText="Confirmar senha">
            <font>
                <Font size="16.0" />
            </font>
        </PasswordField>
        <Button fx:id="cadastrarButton" layoutX="255.0" layoutY="244.0" mnemonicParsing="false" onAction="#cadastrar" text="Cadastrar">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
        <Hyperlink fx:id="loginLink" ellipsisString="" layoutX="275.0" layoutY="338.0" onAction="#logarLink" text="Login">
            <font>
                <Font size="16.0" />
            </font>
        </Hyperlink>
        <Label fx:id="msgCadastrarLabel" layoutX="201.0" layoutY="295.0" prefHeight="25.0" prefWidth="198.0">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
